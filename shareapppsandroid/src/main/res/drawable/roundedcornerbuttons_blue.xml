<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

  <item>
    <shape android:shape="rectangle">
      <solid android:color="#00B0C5" />
      <!--<solid android:color="#DEB887"/>-->
      <stroke android:color="#00B0C5" android:width="2dp" />
      <!--mete arredondados os conrers-->
      <corners android:radius="15dp" />
    </shape>
  </item>

  <item android:drawable="@color/dark_blue" android:state_enabled="true" android:state_pressed="true" />
  <item android:drawable="@color/dark_blue" android:state_enabled="true" android:state_focused="true" />
  <item android:drawable="@color/dark_blue" android:state_enabled="true" android:state_selected="true" />
</selector>