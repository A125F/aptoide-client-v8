<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
  <item>
    <shape android:shape="rectangle" >
      <solid android:color="@color/transparent"/>
    </shape>
  </item>
  <item>
    <rotate
        android:fromDegrees="45"
        android:pivotX="85%"
        android:pivotY="135%">
      <shape>
        <solid android:color="@color/grey_fog_dark" />

      </shape>
    </rotate>
  </item>
  <item>
    <rotate
        android:fromDegrees="45"
        android:pivotX="130%"
        android:pivotY="25%">
      <shape android:shape="rectangle">
        <solid android:color="@color/grey_fog_dark" />

      </shape>
    </rotate>
  </item>
</layer-list>