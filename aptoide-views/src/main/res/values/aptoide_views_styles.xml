    <attr name="skeleton_color" format="reference|color" />
    <attr name="skeleton_border_color" format="reference|color" />
    <attr name="skeleton_border_thickness" format="dimension" />
    <attr name="skeleton_corner_radius" format="dimension" />
  </declare-styleable>
</resources>